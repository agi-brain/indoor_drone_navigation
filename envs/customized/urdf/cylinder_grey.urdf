<?xml version="1.0"?>
<robot name="cylinder_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>

  <link name="cylinder_link">

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.0833333" ixy="0.0" ixz="0.0" iyy="0.0833333" iyz="0.0" izz="0.0"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="1" radius="0.012"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="1" radius="0.012"/>
      </geometry>
    </collision>

  </link>
</robot>

